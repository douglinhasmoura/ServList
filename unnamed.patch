+package com.br.servlist;
+
+import android.app.ProgressDialog;
+import android.content.Context;
+import android.content.Intent;
+import android.os.Bundle;
+import android.support.design.widget.FloatingActionButton;
+import android.support.v7.app.AppCompatActivity;
+import android.view.View;
+import android.widget.EditText;
+import android.widget.TabHost;
+
+import com.github.pierry.simpletoast.SimpleToast;
+
+public class actCad_EmpValidarCpfCnpj extends AppCompatActivity  {
+
+
+    private EditText cnpj, cpf, nomeCpf, nomeCnpj;
+    private FloatingActionButton btnProxCnpj, btnProxCpf;
+    private ProgressDialog dialog;
+    private TabHost abas= null;
+    boolean resCNPJ = false;
+
+
+
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.act_cad_emp_nome_cnpj);
+
+
+        abas = (TabHost)findViewById(R.id.tabhost);
+        abas.setup();
+
+        TabHost.TabSpec descritor = abas.newTabSpec("CNPJ");
+        descritor.setContent(R.id.tab_cnpj);
+        descritor.setIndicator(getString(R.string.tab_cnpj));
+        abas.addTab(descritor);
+
+        descritor = abas.newTabSpec("CPF");
+        descritor.setContent(R.id.tab_cpf);
+        descritor.setIndicator(getString(R.string.tab_cpf));
+        abas.addTab(descritor);
+        
+
+        cnpj = (EditText) findViewById(R.id.cnpj);
+        cpf = (EditText) findViewById(R.id.cpf);
+        nomeCnpj = (EditText) findViewById(R.id.nome_empresa_cnpj);
+        nomeCpf = (EditText)findViewById(R.id.nome_empresa_cpf);
+        btnProxCnpj = (FloatingActionButton) findViewById(R.id.fab_cnpj);
+        btnProxCpf = (FloatingActionButton)findViewById(R.id.fab_cpf);
+
+
+        btnProxCnpj.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View v) {
+
+
+                //validando CNPJ
+
+                Context context = getApplicationContext();
+
+                Validacoes validacoes = new Validacoes();
+                //validda se os campos estão vazios
+
+                if(nomeCnpj.getText().toString().length() == 0) {
+                    nomeCnpj.setError("NOME INVÁLIDO");
+                }
+                if(cnpj.getText().toString().length() == 0) {
+                    cnpj.setError("CNPJ INVÁLIDO");
+                }
+                else {
+
+
+                    dialog = ProgressDialog.show(actCad_EmpValidarCpfCnpj.this, "Validando", "Aguarde ...", false);
+                    resCNPJ = validacoes.isCnpj(cnpj.getText().toString());
+
+                    //se for vdd valida e deixa passar para a próxima página
+                    if (resCNPJ == true) {
+
+                        cpf.setEnabled(false);
+                        SimpleToast.ok(context, "CNPJ VALIDADO COM SUCESSO");
+
+
+                        dialog.dismiss();
+                        Intent intent = new Intent(actCad_EmpValidarCpfCnpj.this, actEmailSenha.class);
+                        startActivity(intent);
+
+                    } else { //se não cnpj inválido e não deixa passar.
+
+                        SimpleToast.error(context, "CNPJ INVÁLIDO", "{fa-exclamation-circle}");
+                        dialog.dismiss();
+                    }
+
+                }
+
+                }
+
+
+        });
+
+        btnProxCpf.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View v) {
+
+                boolean resCpf = false;
+                Context context = getApplicationContext();
+                Validacoes validacoes = new Validacoes();
+
+                if(nomeCpf.getText().toString().length() == 0) {
+                    nomeCpf.setError("NOME INVÁLIDO");
+                }
+                if(cpf.getText().toString().length() == 0) {
+                    cpf.setError("CPF INVÁLIDO");
+                }
+                else {
+
+
+                    dialog = ProgressDialog.show(actCad_EmpValidarCpfCnpj.this, "Validando", "Aguarde ...", false);
+                    resCpf = validacoes.isCPF(cpf.getText().toString());
+
+                    //se for vdd valida e deixa passar para a próxima página
+                    if (resCpf == true) {
+
+                        SimpleToast.ok(context, "CPF VALIDADO COM SUCESSO");
+
+
+                        dialog.dismiss();
+                        Intent intent = new Intent(actCad_EmpValidarCpfCnpj.this, actEmailSenha.class);
+                        startActivity(intent);
+
+                    } else { //se não cnpj inválido e não deixa passar.
+
+                        SimpleToast.error(context, "CPF INVÁLIDO", "{fa-exclamation-circle}");
+                        dialog.dismiss();
+                    }
+
+                }
+
+            }
+
+        });
+
+    }
+
+}
